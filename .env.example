# App runtime
PORT=18090
SPRING_PROFILES_ACTIVE=mysql
STOCKDASH_SEED_ENABLED=true

# MySQL container and app datasource
STOCKDASH_MYSQL_IMAGE=mysql:8.4
STOCKDASH_DB_PORT=3306
STOCKDASH_DB_NAME=stockdash
STOCKDASH_DB_USERNAME=stockdash_app
STOCKDASH_DB_PASSWORD=stockdash_dev_password
STOCKDASH_DB_ROOT_PASSWORD=stockdash_root_password

# Optional external pricing source
STOCKDASH_PRICING_ALPHA_VANTAGE_API_KEY=

# Observability stack
STOCKDASH_PROMETHEUS_IMAGE=prom/prometheus:v2.54.1
STOCKDASH_PROMETHEUS_PORT=9090
STOCKDASH_GRAFANA_IMAGE=grafana/grafana:11.2.2
STOCKDASH_GRAFANA_PORT=3000
STOCKDASH_GRAFANA_ADMIN_USER=admin
STOCKDASH_GRAFANA_ADMIN_PASSWORD=admin
